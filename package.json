  "keywords": [
    "angular",
    "model",
    "rest"
  ],
  "author": "Ben Drucker <bvdrucker@gmail.com> (http://www.bendrucker.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valet-io/ng-base-model/issues"
  },
  "homepage": "https://github.com/valet-io/ng-base-model",
  "devDependencies": {
    "karma": "~0.12.9",
    "karma-phantomjs-launcher": "~0.1.4",
    "mocha": "~1.18.2",
    "karma-mocha": "~0.1.3",
    "karma-chrome-launcher": "~0.1.3",
    "bower": "~1.3.2",
    "browserify": "~3.41.0",
    "karma-browserify": "~0.2.0",
    "chai": "~1.9.1",
    "browserify-shim": "~3.4.1",
    "sinon-chai": "~2.5.0",
    "sinon": "~1.9.1",
    "karma-chai-sinon": "~0.1.3",
    "chai-as-promised": "~4.1.1",
    "es5-shim": "~2.3.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
