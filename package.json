{
  "name": "ng-base-model",
  "version": "0.2.0",
  "description": "Our base model layer for building Angular applications",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "install": "bower install"
